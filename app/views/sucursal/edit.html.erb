<div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Sucursal</h4>
                            </div>
                            <div class="content">
                                <!--<form>-->
                                    <%= form_for @sucursal do |f| %>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label>Rut </label>
                                                <%= f.text_field :rut, class: "form-control", placeholder: "RUT" %>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Nombre Sucursal</label>
                                                <%= f.text_field :nombre, class: "form-control", placeholder: "Nombre sucursal" %>
                                                <!--<input type="text" class="form-control"  placeholder="Nombre sucursal" >-->
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Número Fiscal</label>
                                                <%= f.text_field :nro_fiscal, class: "form-control", placeholder: "Número Fiscal" %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Teléfono</label>

                                                <%= f.text_field :fono1, class: "form-control", placeholder: "Teléfono 1" %>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Teléfono 2</label>
                                                <%= f.text_field :fono2, class: "form-control", placeholder: "Teléfono 2" %>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Giro</label>
                                                <%= f.collection_select(:giro_id, Giro.order('id'), :id, :actividad, { :prompt => 'Seleccione'}, {class: 'form-control'}) %>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Razon social</label>
                                                <%= f.text_field :razon_social, class: "form-control", placeholder: "Razón Social" %>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-10">
                                            <div class="form-group">
                                                <label>Direccion</label>
                                                <%= f.text_field :direccion, class: "form-control", placeholder: "Dirección" %>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Numero calle</label>
                                                <%= f.text_field :numero, class: "form-control", placeholder: "Número Calle" %>
                                            </div>
                                        </div>
                                    </div>

                                    <!--<div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Región</label>
                                                <label>Región</label>
                                                <input type="text" class="form-control" placeholder="Ciudad" >
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Provincia</label>
                                                <select id="provincia" name="provincia" class="form-control">
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Comuna</label>
                                               # < f.collection_select(:comuna_id, Comuna.order('id'), :id, :nombre, { :prompt => 'Seleccione'}, {class: 'form-control'}) %>
                                            </div>
                                        </div>
                                    </div>-->

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Descripcion</label>
                                                <%= f.text_field :descripcion, class: "form-control", placeholder: "Descripción" %>
                                            </div>
                                        </div>
                                    </div>
                                    <%= f.submit value: 'Guardar', class: 'btn btn-info btn-fill pull-right'%>

                                    <div class="clearfix"></div>
                                    <% end %>
                                <!--</form>-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

